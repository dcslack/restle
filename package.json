{
  "name": "restle",
  "version": "0.2.7",
  "description": "JSON API engine.",
  "main": "dist/lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dcslack/restle.git"
  },
  "keywords": [
    "rest",
    "api",
    "express",
    "node",
    "ember-data"
  ],
  "author": "Dylan Slack <dylanslack@gmail.com> (http://dylanslack.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dcslack/restle/issues"
  },
  "homepage": "http://restle.io",
  "dependencies": {
    "babel": "^5.6.14",
    "body-parser": "^1.10.1",
    "commander": "^2.8.1",
    "express": "^4.7.2",
    "i": "^0.3.3",
    "lodash": "^2.4.1",
    "mongoose": "^3.8.21",
    "prettyjson": "^1.1.2",
    "string-template": "^0.2.1",
    "tape": "^4.0.0"
  },
  "bin": {
    "restle": "./bin/restle"
  },
  "devDependencies": {
    "blue-tape": "^0.1.10",
    "eslint": "^0.24.1",
    "mkdirp": "^0.5.1",
    "mocha": "^2.2.5",
    "mongojs": "^1.0.2",
    "prettyjson": "^1.1.2",
    "rimraf": "^2.4.0",
    "supertest": "^1.0.1",
    "tap-spec": "^4.0.2",
    "tapes": "^2.0.0"
  },
  "scripts": {
    "clean": "rimraf dist coverage",
    "prepublish": "npm run clean && npm run build",
    "build": "npm run build:lib && npm run build:test",
    "build:lib": "mkdirp dist/lib && babel lib --out-dir dist/lib",
    "build:test": "mkdirp dist/test && babel test --out-dir dist/test",
    "test": "npm run test:lint && npm run test:server",
    "test:lint": "eslint lib test",
    "test:server": "npm run clean && npm run build && tape dist/test | tap-spec",
    "test:coverage": "npm run clean && npm run build && istanbul cover dist/test | tap-spec"
  },
  "files": [
    "dist/lib",
    "dist/test",
    "lib/",
    "test/",
    "bin/",
    "LICENSE"
  ]
}
